{
  "name": "interview-assignment",
  "version": "1.0.0",
  "description": "bigstack interview-assignment",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "dev": "run-p dev:client dev:server",
    "dev:client": "cross-env NODE_ENV=development webpack serve",
    "dev:server": "cross-env NODE_ENV=development nodemon --watch server .",
    "build": "yarn run clean && cross-env webpack",
    "build:analyze": "cross-env ANALYZE_MODE=true yarn run build",
    "start": "cross-env NODE_ENV=production node .",
    "lint": "run-s lint:js lint:style",
    "lint:fix": "run-s lint:js:fix lint:style:fix",
    "lint:js": "eslint .",
    "lint:style": "stylelint **/*.scss --custom-syntax postcss-scss",
    "lint:js:fix": "eslint . --fix",
    "lint:style:fix": "stylelint **/*.scss --fix --custom-syntax postcss-scss",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "yarn": ">=1.22.19",
    "node": ">=12.13.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE 10"
  ],
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "keywords": [
    "react",
    "express"
  ],
  "license": "MIT",
  "dependencies": {
    "@carbon/charts-react": "^1.11.21",
    "@carbon/ibm-products": "2.3.1",
