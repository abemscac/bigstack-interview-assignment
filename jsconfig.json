{
  "compilerOptions": {
    "target": "ES2020",
    "moduleResolution": "Bundler",
    "paths": {
      "@apis/*": ["./src/apis/*"],
      "@components/*": ["./src/components/*/index.js"],
      "@css-utilities/*": ["./src/css-utilities/*"],
      "@images/*": ["./src/images/*"],
      "@pages/*": ["./src/pages/*/index.js"],
      "@server-data/*": ["./server/data/*"],
      "@utilities/*": ["./src/utilities/*"]
    }
  }
}
